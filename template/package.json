{
  "name": "{{#if_eq scope 'common'}}@strongsoft/{{/if_eq}}{{ name }}",
  "description": "{{ description }}",
  "version": "{{ version }}",
  "author": {
    "name": "{{ authorFullNameFrom author }}",
    "email": "{{ authorEmailFrom author }}"
  },
  "keywords": [
    "vue"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "fix": "npm run lint -- --fix"
  },
  {{#unless_eq githubAccount ""}}
  "repository": {
    "type": "git",
    "url": "git+http://git.strongsoft.net:6280/{{ githubAccount }}/{{ name }}.git"
  },
  "bugs": {
    "url": "http://git.strongsoft.net:6280/{{ githubAccount }}/{{ name }}/issues"
  },
  "homepage": "http://git.strongsoft.net:6280/{{ githubAccount }}/{{ name }}#readme",
  {{/unless_eq}}
  "dependencies": {
    {{#vux}}
    "vux": "^2.9.0",
    {{/vux}}
    "vue": "^2.5.11"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.4",
    "url-loader": "^0.5.8",
    "json-loader": "^0.5.4",
    "style-loader": "0.13.1",
    {{#if_eq css "less"}}
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    {{/if_eq}}
    {{#if_eq css "sass"}}
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.0",
    {{/if_eq}}
    {{#if_eq css "stylus"}}
    "stylus": "^0.54.5",
    "stylus-loader": "^2.4.0",
    {{/if_eq}} 
    {{#vux}}
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "vux-loader": "^1.0.56",
    {{/vux}}
    "vue-loader": "^13.0.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.4",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "opn": "^5.1.0",
    "chalk": "^2.0.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.3",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^2.0.0",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-dev-server": "^2.9.1"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}